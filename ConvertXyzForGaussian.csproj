<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Mono.CSharp" Version="4.0.0.143" />
    <PackageReference Include="System.Reflection.Metadata" Version="9.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="com\**" />
    <Compile Remove="ubuntu\**" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="com\**" />
    <EmbeddedResource Remove="ubuntu\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="com\**" />
    <None Remove="ubuntu\**" />
  </ItemGroup>

</Project>
